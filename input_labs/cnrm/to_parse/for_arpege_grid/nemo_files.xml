      <file_group id="5d" output_freq="5d"  output_level="10" enabled=".TRUE."/>  <!-- 5d files -->  
      <file_group id="1m" output_freq="1mo" output_level="10" enabled=".TRUE."> <!-- real monthly files -->

        <file id="file3" name_suffix="_grid_T" >
	  <field field_ref="e3t" 	  name="thkcello"  long_name="T-cell thickness" />
	  <field field_ref="masscello" 	  name="masscello" long_name="sea_water_mass" />
          <field field_ref="botpres"      name="pbo"      long_name="Pressure_at_sea_floor"                                          />
          <!-- pso : sea_water_pressure_at_sea_water_surface = 0 -->
          <field field_ref="sshdyn"       name="zos"      long_name="sea_surface_height_above_geoid"                                 />
          <field field_ref="sshdyn2"      name="zossq"    long_name="square_of_sea_surface_height_above_geoid"             level="2" />
	  <field field_ref="sshdyn"       name="zosstd"   long_name="sea_surface_height_above_geoid_standard_deviation"  operation="instant" freq_op="1d" > sqrt( @sshdyn2 - @sshdyn * @sshdyn ) </field>

          <field field_ref="toce"         name="bigthetao" long_name="sea_water_conservative_temperature"   operation="average" freq_op="1mo" > @toce_e3t / @e3t </field>     
          <field field_ref="toce_pot"     name="thetao"   long_name="sea_water_potential_temperature"   operation="average" freq_op="1mo" > @toce_pot_e3t / @e3t </field>       
          <field field_ref="sst"          name="tos"      long_name="sea_surface_temperature"                              level="1" />
          <field field_ref="sst2"         name="tossq"    long_name="square_of_sea_surface_temperature"                    level="2" />
	  <field field_ref="sst"          name="tosstd"   long_name="sea_surface_temperature_standard_deviation"         operation="instant" freq_op="1d" > sqrt( @sst2 - @sst * @sst ) </field>       
          <field field_ref="sbt"          name="tob"      long_name="sea_water_potential_temperature_at_sea_floor"         level="1" />
          <!-- no agessc : sea_water_age_since_surface_contact -->
          <!-- no cfc11  : moles_per_unit_mass_of_cfc11_in_sea_water -->
          <!-- msftbarot : ocean_barotropic_mass_streamfunction : offline -->
          <!-- mlotst    :           ocean_mixed_layer_thickness_defined_by_sigma_t : must be done offline -->
          <!-- mlotstsq  : square_of_ocean_mixed_layer_thickness_defined_by_sigma_t : must be done offline -->
          <field field_ref="qns"           name="nshfls"      long_name="surface_net_downward_non_solar_flux"                                         level="1" />
          <field field_ref="qns_tot"                                                                                                                  level="1" />
          <field field_ref="qnsrcv"                                                                                                                   level="1" />
          <field field_ref="hflx_evap_cea" name="hfevapds"    long_name="temperature_flux_due_to_evaporation_expressed_as_heat_flux_out_of_sea_water" level="1" />
          <field field_ref="hflx_cal_cea"  name="hfibthermds" long_name="heat_flux_into_sea_water_due_to_iceberg_thermodynamics"                      level="1" />
          <field field_ref="hflx_snow_cea" name="qnssnw"                                                                                              level="1" />
          <field field_ref="hflx_rnf_cea"  name="hfrunoffds"  long_name="temperature_flux_due_to_runoff_expressed_as_heat_flux_into_sea_water"        level="1" />
          <field field_ref="hflx_rain_cea" name="hfrainds"    long_name="temperature_flux_due_to_rainfall_expressed_as_heat_flux_into_sea_water" level="1" > @hflx_rain_cea + @hflx_snow_cea </field>
          <field field_ref="qsr"           name="rsntds"      long_name="surface_net_downward_shortwave_flux"                                         level="1" />
          <field field_ref="qsr_tot"                                                                                                                  level="1" />
          <field field_ref="qsrrcv"                                                                                                                  level="1" />
          <field field_ref="qsr3d"         name="rsds"        long_name="downwelling_shortwave_flux_in_sea_water"                                     level="1" />
	  <field field_ref="heatc"         name="heatc"       long_name="Heat content vertically integrated"            />
          <field field_ref="qns"           name="hfds"        long_name="surface_downward_heat_flux_in_sea_water"  level="1" > @qt + @hflx_rnf_cea </field>
          <field field_ref="sst_m"        name="sst_m"    long_name="averaged_sea_surface_temperature"                     level="1" />
          <field field_ref="sss_m"        name="sss_m"    long_name="averaged_sea_surface_salinity"                        level="1" />
          <field field_ref="qoc_m"        name="qoc_m"   long_name="averaged_sea_surface_temperature_xlevels"             level="1" />
          <!-- next variables available with key_diahth -->
          <field field_ref="20d"          name="z20d"     long_name="depth_of_isosurface_of_sea_water_potential_temperature"  level="2" />
          <field field_ref="28d"          name="z28d"     long_name="depth_of_isosurface_of_sea_water_potential_temperature"  level="2" />
          <field field_ref="hc300"        name="hc300"   long_name="integral_of_sea_water_potential_temperature_wrt_depth_expressed_as_heat_content"   level="2"  />

         </file>

        <file id="file4" name_suffix="_grid_S" >
	  <field field_ref="e3t" long_name="T-cell thickness" />
          <field field_ref="soce"         name="so"       long_name="sea_water_salinity"                operation="average" freq_op="1mo" > @soce_e3t / @e3t </field>       
          <field field_ref="sss"          name="sos"      long_name="sea_surface_salinity"                                 level="1" />
          <field field_ref="sbs"          name="sob"      long_name="sea_water_salinity_at_sea_floor"                                 level="1" />
	  <field field_ref="saltc"        name="saltc"    long_name="Salt content vertically integrated"		             />                         
          <field field_ref="rhop"         name="rhopoto"  long_name="sea_water_potential_density"                             level="2" />

          <field field_ref="empmr"        name="wfo"      long_name="water_flux_into_sea_water"                               level="1" />
	  <!--field_ref="erp"          name="wfcorr"   long_name="water_flux_correction"	                              level="1" -->
          <!-- wfonocorr : water_flux_into_sea_water_without_flux_correction : emp - erp -->	  
          <field field_ref="empmr"        name="wfnocorr" long_name="water_flux_into_sea_water_without_flux_correction"       level="1" />
          <field field_ref="emp_gel"      name="evs"      long_name="water_evaporation_flux"		                      level="1" > @emp_gel - @pr - @prsn </field>
          <field field_ref="calving_cea"  name="ficeberg" long_name="water_flux_into_sea_water_from_icebergs"                 level="1" />
          <field field_ref="runoffs"      name="friver"   long_name="water_flux_into_sea_water_from_rivers"                   level="1" />

          <field field_ref="mldkz5"       name="omlmax"   long_name="ocean_mixed_layer_thickness_defined_by_mixing_scheme" level="2" operation="maximum" />
          <field field_ref="mldkz5"       name="mldkz5"   level="2"  />
          <field field_ref="mldr10_1"     name="mldr10_1" level="2" />
          <field field_ref="mldr10_1max"  name="mldr10_1max"  long_name="max of Mixed Layer Depth 0.01 ref.10m"              operation="maximum" />
          <field field_ref="mldr10_1min"  name="mldr10_1min"  long_name="min of Mixed Layer Depth 0.01 ref.10m"              operation="minimum" />          
          <field field_ref="acalving_cea" name="fcalva"    long_name="antarctic_calving"                                     level="1" /> <!-- usually = 0 -->
          <field field_ref="gcalving_cea" name="fcalvg"    long_name="groenland_calving"                                     level="1" /> <!-- usually = 0 -->
          <field field_ref="lake"         name="flake"     long_name="lake_water_budget"                                     level="1" /> <!-- usually = 0 -->
          <field field_ref="tevp"                                                                                            level="1" /> <!-- usually = 0 -->
          <field field_ref="tprec"                                                                                           level="1" /> <!-- usually = 0 -->
          <field field_ref="e-p"                                                                                             level="1" /> <!-- usually = 0 -->
          <!-- next variables available with key_diahth -->
          <field field_ref="mldr0_3"    name="mlotst"     long_name="ocean_mixed_layer_thickness_defined_by_sigma_t"	     level="2" />
          <field field_ref="mldr0_3"    name="mlotstmax"  long_name="ocean_mixed_layer_thickness_defined_by_sigma_t"	     level="2" operation="maximum" />
          <field field_ref="mldr0_3"    name="mlotstmin"  long_name="ocean_mixed_layer_thickness_defined_by_sigma_t"	     level="2" operation="minimum" />
        </file>


