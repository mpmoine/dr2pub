      <field id="TACod550ss" long_name="atmosphere_optical_thickness_at_550nm_due_to_seasalt_ambient_aerosol" unit="1" 
             > TACod550ss_bin1 + TACod550ss_bin2 + TACod550ss_bin3 </field>
      <field id="TACod550dust_bin1" long_name="atmosphere_optical_thickness_at_550nm_due_to_dust_bin1_ambient_aerosol" unit="1" />
      <field id="TACod550dust_bin2" long_name="atmosphere_optical_thickness_at_550nm_due_to_dust_bin2_ambient_aerosol" unit="1" />
      <field id="TACod550dust_bin3" long_name="atmosphere_optical_thickness_at_550nm_due_to_dust_bin3_ambient_aerosol" unit="1" />
      <field id="TACod550dust" long_name="atmosphere_optical_thicknedust_at_550nm_due_to_dust_ambient_aerosol" unit="1" 
             > TACod550dust_bin1 + TACod550dust_bin2 + TACod550dust_bin3 </field>
      <field id="TACod550oa_phil" long_name="atmosphere_optical_thickness_at_550nm_due_to_hydrophilic_particulate_organic_matter_ambient_aerosol" unit="1" />
      <field id="TACod550oa_phob" long_name="atmosphere_optical_thickness_at_550nm_due_to_hydrophobic_particulate_organic_matter_ambient_aerosol" unit="1" />
      <field id="TACod550oa" long_name="atmosphere_optical_thickness_at_550nm_due_to_particulate_organic_matter_ambient_aerosol" unit="1" 
             > TACod550oa_phil + TACod550oa_phob </field>
      <field id="TACod550bc_phil" long_name="atmosphere_optical_thickness_at_550nm_due_to_hydrophilic_black_carbon_ambient_aerosol" unit="1" />
      <field id="TACod550bc_phob" long_name="atmosphere_optical_thickness_at_550nm_due_to_hydrophobic_black_carbon_ambient_aerosol" unit="1" />
      <field id="TACod550bc" long_name="atmosphere_optical_thickness_at_550nm_due_to_black_carbon_ambient_aerosol" unit="1" 
