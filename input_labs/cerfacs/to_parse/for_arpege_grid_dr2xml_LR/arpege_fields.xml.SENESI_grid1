      <field id="TACod440so4" long_name="atmosphere_optical_thickness_at_440nm_due_to_sulfate_ambient_aerosol" unit="1" />
      <field id="TACod440aer" long_name="atmosphere_optical_thickness_at_440nm_due_to_ambient_aerosol" unit="1" 
             > TACod440ss + TACod440dust + TACod440oa + TACod440bc + TACod440so4 </field>
      
      <field id="TACod550ss_bin1" long_name="atmosphere_optical_thickness_at_550nm_due_to_seasalt_bin1_ambient_aerosol" unit="1" />
      <field id="TACod550ss_bin2" long_name="atmosphere_optical_thickness_at_550nm_due_to_seasalt_bin2_ambient_aerosol" unit="1" />
