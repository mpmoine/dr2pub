      <field id="TACod865so4" long_name="atmosphere_optical_thickness_at_865nm_due_to_sulfate_ambient_aerosol" unit="1" />
      <field id="TACod865aer" long_name="atmosphere_optical_thickness_at_865nm_due_to_ambient_aerosol" unit="1" 
