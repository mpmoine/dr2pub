<!-- Declaration of Arpege fields , and some derived fields

Field long name and units are declared here 

1st version : S.Senesi - 05/2016 : 
Does not include chemical fields, nor all interesting fields
Changes 
08/2016 - S.Senesi : put long_names and units here
-->


<field_definition level="1" prec="8" operation="instant"  freq_op="1ts" default_value="1.e+20">

  <field_group grid_ref="FULL_grid" >
    <field_group id="arpege_instant_2d" >
      <field id="psl"     long_name="air_pressure_at_sea_level" unit="Pa"/>
      <field id="ps"      long_name="surface_air_pressure" unit="Pa"/>
      <!-- Next field is an example of a field sent by actqsat -->
      <field id="thetaws" long_name="lowest_level_pseudo_adiabatic_wet_bulb_temperature" unit="K"/>
    </field_group>

    <field_group id="arpege_fluxes" operation="average" freq_op="1ts">
      <!-- These fields should undergo HF sampling and time averaging -->
      <field id="clt"     long_name="cloud_area_fraction" unit="1"/>
      <field id="clt100"  long_name="cloud_area_fraction" unit="%" field_ref="clt"> clt * 100 </field>
      <field id="cltc"    long_name="convective_cloud_area_fraction" unit="1"/>
      <field id="cltc100" long_name="convective_cloud_area_fraction" unit="%" field_ref="cltc"> cltc * 100  </field>
      <field id="clth"    long_name="cloud_area_fraction_in_atmosphere_layer_high" unit="1"/>
      <field id="clth100" long_name="cloud_area_fraction_in_atmosphere_layer_high" unit="%" field_ref="clth"> clth * 100 </field>
      <field id="cltm"    long_name="cloud_area_fraction_in_atmosphere_layer_middle" unit="1"/>
      <field id="cltm100" long_name="cloud_area_fraction_in_atmosphere_layer_middle" unit="%" field_ref="cltm"> cltm * 100 </field>
      <field id="cltl"    long_name="cloud_area_fraction_in_atmosphere_layer_low" unit="1"/>
      <field id="cltl100" long_name="cloud_area_fraction_in_atmosphere_layer_low" unit="%" field_ref="cltl"> cltl * 100 </field>
      <field id="clwc"    long_name="atmosphere_mass_content_of_cloud_liquid_water" unit="kg m-2"/>
      <field id="clivi"   long_name="atmosphere_mass_content_of_cloud_ice_water" unit="kg m-2"/>
      <field id="prw"     long_name="precipitable_water" unit="kg m-2"/>
      <!-- <field id="ozont"   long_name="ozone total" unit="Dbs" field_ref="ATMOOZONE TOTALE" name="ozont" /> -->
      <field id="prlic"   long_name="convective liquid precipitation" unit="kg m-2 s-1"/>
      <field id="prsnc"   long_name="convective solid precipitation" unit="kg m-2 s-1"/>
      <field id="prlils"  long_name="large scale liquid precipitation" unit="kg m-2 s-1"/>
      <field id="prsnls"  long_name="large scale solid precipitation" unit="kg m-2 s-1"/>
      <field id="rst"     long_name="Radiation Shortwave Top" unit="W m-2"/>
      <field id="rlut" 	  long_name="Radiation Longwave Up Top" unit="W m-2"/>
      <field id="rsdt" 	  long_name="Radiation Shortwave Down Top" unit="W m-2"/>
      <field id="rstcs"   long_name="Radiation Shortwave Top Clear Sky" unit="W m-2"/>
      <field id="rlutcs"  long_name="Radiation Longwave Up Top Clear Sky" unit="W m-2"/>
      <field id="rlscs"   long_name="Radiation Longwave Surface Clear Sky" unit="W m-2"/>
      <field id="rsscs"   long_name="Radiation Shortwave Surface Clear Sky" unit="W m-2"/>
      <field id="rsdscs"  long_name="Radiation Shortwave Downward Surface Clear Sky" unit="W m-2"/>
      <field id="rldscs"  long_name="Radiation Longwave Surface Clear Sky" unit="W m-2"/>
      <field id="FSU_ZBUDSO" long_name="Shortwave radiation budget correction" unit="W m-2"/>
      <field id="FSU_ZBUDTH" long_name="Longwave radiation budget correction" unit="W m-2"/>
      <field id="rlscln"    long_name="Clean Radiation Longwave Surface" unit="W m-2"/>
      <field id="rsscln"    long_name="Clean Radiation Shortwave Surface" unit="W m-2"/>
      <field id="rstcln"    long_name="Clean Radiation Shortwave Top" unit="W m-2"/>
      <field id="rlutcln"   long_name="Clean Radiation Longwave Up Top" unit="W m-2"/>
      <field id="rstcscln"  long_name="Clean Radiation Shortwave Top Clear Sky" unit="W m-2"/>
      <field id="rlutcscln" long_name="Clean Radiation Longwave Up Top Clear Sky" unit="W m-2"/>
      <field id="rlscscln"  long_name="Clean Radiation Longwave Surface Clear Sky" unit="W m-2"/>
      <field id="rsscscln"  long_name="Clean Radiation Shortwave Surface Clear Sky" unit="W m-2"/>
      <field id="rlsbis"    long_name="Radiation Longwave Surface for second aerosol climatology" unit="W m-2"/>
      <field id="rssbis"    long_name="Radiation Shortwave Surface for second aerosol climatology" unit="W m-2"/>
      <field id="rstbis"    long_name="Radiation Shortwave Top for second aerosol climatology" unit="W m-2"/>
      <field id="rlutbis"   long_name="Radiation Longwave Up Top for second aerosol climatology" unit="W m-2"/>
      <field id="rstcsbis"  long_name="Radiation Shortwave Top Clear Sky for second aerosol climatology" unit="W m-2"/>
      <field id="rlutcsbis" long_name="Radiation Longwave Up Top Clear Sky for second aerosol climatology" unit="W m-2"/>
      <field id="rlscsbis"  long_name="Radiation Longwave Surface Clear Sky for second aerosol climatology" unit="W m-2"/>
      <field id="rsscsbis"  long_name="Radiation Shortwave Surface Clear Sky for second aerosol climatology" unit="W m-2"/>
    </field_group>
    
    <field_group id="arpege_instant_3d" freq_op="6h" grid_ref="FULL_klev" >
      <field id="pa"   name="pa_m"  long_name="pressure" unit="Pa"/>
      <field id="zg"   name="zg_m"  long_name="geopotential height" unit="m">this/$gravit </field>
      <field id="ta"   name="ta_m"  long_name="air_temperature" unit="K"/>
      <field id="ua"   name="ua_m"  long_name="eastward_wind" unit="m s-1"/>
      <field id="va"   name="va_m"  long_name="northward" unit="m s-1"/>
      <field id="hus"  name="hus_m" long_name="specific_humidity" unit="1"/>
      <field id="hur"  name="hur_m" long_name="relative_humidity" unit="%"/>
      <field id="wap"  name="wap_m" long_name="lagrangian_tendency_of_air_pressure" unit="Pa s-1"/>
      <field id="tro3" name="ozo_m" long_name="mole_fraction_of_ozone_in_air" unit="1"/>
    </field_group>

    <field_group id="arpege_3dp" grid_ref="FULL_pstd" freq_op="6h" detect_missing_value="true" >
      <field id="zg_p"  field_ref="zg"   name="zg"  />
      <field id="ta_p"  field_ref="ta"   name="ta"  />
      <field id="ua_p"  field_ref="ua"   name="ua"  />
      <field id="va_p"  field_ref="va"   name="va"  />
      <field id="hus_p" field_ref="hus"  name="hus" />
      <field id="hur_p" field_ref="hur"  name="hur" />
      <field id="wap_p" field_ref="wap"  name="wap" />
      <field id="ozo_p" field_ref="tro3" name="ozo" />
    </field_group>

    <field_group id="arpege_derived" freq_op="1ts" operation="average">
      <field id="prli"   name="prli"   field_ref="prlic"  long_name="liquid precipitation" 
	     unit="kg m-2 s-1" > prlic + prlils </field>
      <field id="prsn"   name="prsn"   field_ref="prsnc"  long_name="solid precipitation" 
	     unit="kg m-2 s-1" > prsnc + prsnls </field>
      <field id="prc"    name="prc"    field_ref="prlic"  long_name="convective precipitation" 
	     unit="kg m-2 s-1" > prlic + prsnc  </field>
      <field id="prls"   name="prls"   field_ref="prlils" long_name="large scale precipitation" 
	     unit="kg m-2 s-1" > prlils + prsnls </field>
      <field id="pr"     name="pr"     field_ref="prc"    long_name="total precipitation" 
	     unit="kg m-2 s-1" > prc + prls </field>
      <field id="clwvi"  name="clwvi"  field_ref="clivi"  long_name="TBD" unit="kg m-2"     > clivi + clwc    </field>

      <field id="rsut"   name="rsut"   field_ref="rsdt"   long_name="TBD" unit="W m-2"      > rsdt - rst      </field>
      <field id="rsutcs" name="rsutcs" field_ref="rsdt"   long_name="TBD" unit="W m-2"      > rsdt - rstcs    </field>
      <field id="rsuscs" name="rsuscs" field_ref="rsdscs" long_name="TBD" unit="W m-2"      > rsdscs + rsscs  </field>
      <field id="rluscs" name="rluscs" field_ref="rldscs" long_name="TBD" unit="W m-2"      > rldscs + rlscs </field>
      <field id="rtmt"   name="rtmt"   field_ref="rst"    long_name="TBD" unit="W m-2"      > rst - rlut      </field>
    </field_group>

    <field_group id="aero_prog_2D" freq_op="1ts" operation="instant">
      
      <field id="TACod440ss_bin1" long_name="atmosphere_optical_thickness_at_440nm_due_to_seasalt_bin1_ambient_aerosol" unit="1" />
      <field id="TACod440ss_bin2" long_name="atmosphere_optical_thickness_at_440nm_due_to_seasalt_bin2_ambient_aerosol" unit="1" />
      <field id="TACod440ss_bin3" long_name="atmosphere_optical_thickness_at_440nm_due_to_seasalt_bin3_ambient_aerosol" unit="1" />
      <field id="TACod440ss" long_name="atmosphere_optical_thickness_at_440nm_due_to_seasalt_ambient_aerosol" unit="1" 
             > TACod440ss_bin1 + TACod440ss_bin2 + TACod440ss_bin3 </field>
      <field id="TACod440dust_bin1" long_name="atmosphere_optical_thickness_at_440nm_due_to_dust_bin1_ambient_aerosol" unit="1" />
      <field id="TACod440dust_bin2" long_name="atmosphere_optical_thickness_at_440nm_due_to_dust_bin2_ambient_aerosol" unit="1" />
      <field id="TACod440dust_bin3" long_name="atmosphere_optical_thickness_at_440nm_due_to_dust_bin3_ambient_aerosol" unit="1" />
      <field id="TACod440dust" long_name="atmosphere_optical_thicknedust_at_440nm_due_to_dust_ambient_aerosol" unit="1" 
             > TACod440dust_bin1 + TACod440dust_bin2 + TACod440dust_bin3 </field>
      <field id="TACod440oa_phil" long_name="atmosphere_optical_thickness_at_440nm_due_to_hydrophilic_particulate_organic_matter_ambient_aerosol" unit="1" />
      <field id="TACod440oa_phob" long_name="atmosphere_optical_thickness_at_440nm_due_to_hydrophobic_particulate_organic_matter_ambient_aerosol" unit="1" />
      <field id="TACod440oa" long_name="atmosphere_optical_thickness_at_440nm_due_to_particulate_organic_matter_ambient_aerosol" unit="1" 
             > TACod440oa_phil + TACod440oa_phob </field>
      <field id="TACod440bc_phil" long_name="atmosphere_optical_thickness_at_440nm_due_to_hydrophilic_black_carbon_ambient_aerosol" unit="1" />
      <field id="TACod440bc_phob" long_name="atmosphere_optical_thickness_at_440nm_due_to_hydrophobic_black_carbon_ambient_aerosol" unit="1" />
      <field id="TACod440bc" long_name="atmosphere_optical_thickness_at_440nm_due_to_black_carbon_ambient_aerosol" unit="1" 
