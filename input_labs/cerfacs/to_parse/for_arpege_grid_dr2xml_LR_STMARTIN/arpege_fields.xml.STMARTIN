<!-- Declaration of ARPEGE fields

>> 4 grands groupes de variables que l'on distingue par la fréquence d'échantillonage
   plusieurs sous-groupes thématiques à l'intérieur de ces grands groupes
   pour une utilisation souple dans le arpsfx_file (exemple ci-dessous)

	ARPEGE_2D_instant_variables
        <field_group group_ref="arpege_2D_instant_misc_variables"		enabled="false" />
        <field_group group_ref="arpege_2D_instant_cloud_variables"		enabled="false" />
	<field_group group_ref="arpege_2D_instant_aero_prog"			enabled="false" />

	ARPEGE_2D_avg_variables
      	<field_group group_ref="arpege_2D_avg_misc_variables"			enabled="false" />
      	<field_group group_ref="arpege_2D_avg_radiative_variables"		enabled="false" />
      	<field_group group_ref="arpege_2D_avg_precipitation_variables"		enabled="false" />
      	<field_group group_ref="arpege_2D_avg_cloud_variables"			enabled="false" />
      	<field_group group_ref="arpege_2D_avg_vertic_integrated_fluxes"		enabled="false" />
      	<field_group group_ref="arpege_2D_avg_radiative_clean_variables"	enabled="false" />
      	<field_group group_ref="arpege_2D_avg_radiative_bis_variables"		enabled="false" />
      	<field_group group_ref="arpege_2D_avg_radiative_4xc_variables"		enabled="false" />
	<field_group group_ref="arpege_2D_avg_aero_prog_fluxes"			enabled="false" />

	ARPEGE_3D_instant_variables
      	<field_group group_ref="arpege_3D_instant_mlev_standard"		enabled="false" />
      	<field_group group_ref="arpege_3D_instant_plev_standard"		enabled="false" />
      	<field_group group_ref="arpege_3D_instant_mlev_misc"			enabled="false" />
      	<field_group group_ref="arpege_3D_instant_mlev_t0"			enabled="false" />
      	<field_group group_ref="arpege_3D_instant_plev_misc"			enabled="false" />
      	<field_group group_ref="arpege_3D_instant_mlev_cloud_variables"		enabled="false" />
      	<field_group group_ref="arpege_3D_instant_plev_cloud_variables"		enabled="false" />
      	<field_group group_ref="arpege_3D_instant_mlev_aero_prog" 		enabled="false" />
      	<field_group group_ref="arpege_3D_instant_plev_aero_prog" 		enabled="false" />

	ARPEGE_3D_avg_variables
        <field_group group_ref="arpege_3D_avg_mlev_convective_scheme"		enabled="false" />
      	<field_group group_ref="arpege_3D_avg_mlev_tendencies"			enabled="false" />
      	<field_group group_ref="arpege_3D_avg_plev_tendencies"			enabled="false" />
      	<field_group group_ref="arpege_3D_avg_mlev_rad_fluxes_profiles"		enabled="false" />

	ARPEGE_COSP_variables
      	<field_group group_ref="arpege_calipso"					enabled="false" />
      	<field_group group_ref="arpege_parasol"					enabled="false" />
      	<field_group group_ref="arpege_radar"					enabled="false" />
      	<field_group group_ref="arpege_lidarradar"				enabled="false" />
      	<field_group group_ref="arpege_isccp" 					enabled="false" />
      	<field_group group_ref="arpege_modis"					enabled="false" />
      	<field_group group_ref="arpege_misr"					enabled="false" />
-->


<field_definition level="1" prec="4" operation="instant"  freq_op="1ts" >

  <field_group domain_ref="FULL" >

<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
<!-- 2D ARPEGE fields : instantaneous sampling										-->
<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <field_group id="ARPEGE_2D_instant_variables" operation="instant" freq_op="1ts" detect_missing_value="true" default_value="1.e+20" >
<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->	    
      <field_group id="arpege_2D_instant_misc_variables" >
        <field id="psl"     	long_name="air_pressure_at_sea_level" 					unit="Pa"/>
        <field id="ps"      	long_name="surface_air_pressure" 					unit="Pa"/>
        <field id="airmass" 	long_name="atmosphere_mass_of_air_per_unit_area" 			unit="kg m-2" field_ref="ps"> ps / $gravit </field>
      </field_group>
<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
      <field_group id="arpege_2D_instant_cloud_variables" >
        <field id="lcod"	long_name="liquid_cloud_optical_depth"					unit="1"/>
        <field id="icod"	long_name="ice_cloud_optical_depth"					unit="1"/>
        <field id="cod"		long_name="cloud_optical_depth"						unit="1"> lcod + icod </field>
      </field_group>
<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
