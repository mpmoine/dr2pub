<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
<!-- 2D ARPEGE fields : averaged sampling										-->
<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <field_group id="ARPEGE_2D_avg_variables" operation="average" freq_op="1ts" detect_missing_value="true" default_value="1.e+20" >
<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->	    
      <field_group id="arpege_2D_avg_misc_variables" >
        <field id="toz"		long_name="ozone total" 							unit="Dbs" />
        <field id="tauupbl"	long_name="surface_downward_eastward_stress_due_to_planetary_boundary_layer"	unit="Pa" />
	<field id="tauvpbl"	long_name="surface_downward_northward_stress_due_to_planetary_boundary_layer"	unit="Pa" />
        <field id="ci"		long_name="convection_time_fraction"                     			unit="1" />
      </field_group>
<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
      <field_group id="arpege_2D_avg_radiative_variables" >
        <field id="rst"		long_name="Radiation Shortwave Top"					unit="W m-2"/>
        <field id="rlut"	long_name="Radiation Longwave Up Top"					unit="W m-2"/>
        <field id="rsdt"	long_name="Radiation Shortwave Down Top"				unit="W m-2"/>
        <field id="rstcs"	long_name="Radiation Shortwave Top Clear Sky"				unit="W m-2"/>
        <field id="rlutcs"	long_name="Radiation Longwave Up Top Clear Sky"				unit="W m-2"/>
        <field id="rlscs"	long_name="Radiation Longwave Surface Clear Sky"			unit="W m-2"/>
        <field id="rsscs"	long_name="Radiation Shortwave Surface Clear Sky"			unit="W m-2"/>
        <field id="rsdscs"	long_name="Radiation Shortwave Downward Surface Clear Sky"		unit="W m-2"/>
        <field id="rldscs"	long_name="Radiation Longwave Surface Clear Sky"			unit="W m-2"/>
        <field id="rsut"	long_name="TBD"								unit="W m-2" field_ref="rsdt" > rsdt - rst </field>
        <field id="rsutcs"	long_name="TBD"								unit="W m-2" field_ref="rsdt" > rsdt - rstcs </field>
        <field id="rsuscs"	long_name="TBD"								unit="W m-2" field_ref="rsdscs" > rsdscs + rsscs </field>
        <field id="rluscs"	long_name="TBD"								unit="W m-2" field_ref="rldscs" > rldscs + rlscs </field>
        <field id="rtmt"	long_name="TBD"								unit="W m-2" field_ref="rst" > rst - rlut </field>
        <field id="FSU_ZBUDSO"	long_name="Shortwave radiation budget correction"			unit="W m-2"/>
        <field id="FSU_ZBUDTH"	long_name="Longwave radiation budget correction"			unit="W m-2"/>
      </field_group>
<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
      <field_group id="arpege_2D_avg_precipitation_variables" >
        <field id="prlic"	long_name="convective liquid precipitation"				unit="kg m-2 s-1"/>
        <field id="prsnc"	long_name="convective solid precipitation"				unit="kg m-2 s-1"/>
        <field id="prlils"	long_name="large scale liquid precipitation"				unit="kg m-2 s-1"/>
        <field id="prsnls"	long_name="large scale solid precipitation"				unit="kg m-2 s-1"/>
        <field id="prli"	long_name="liquid precipitation"					unit="kg m-2 s-1" field_ref="prlic" > prlic + prlils </field>
        <field id="prsn"	long_name="solid precipitation"						unit="kg m-2 s-1" field_ref="prsnc" > prsnc + prsnls </field>
        <field id="prc"		long_name="convective precipitation"					unit="kg m-2 s-1" field_ref="prlic" > prlic + prsnc  </field>
        <field id="prls"	long_name="large scale precipitation"					unit="kg m-2 s-1" field_ref="prlils" > prlils + prsnls </field>
        <field id="pr"		long_name="total precipitation"						unit="kg m-2 s-1" field_ref="prc" > prc + prls </field>
      </field_group>
<!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
      <field_group id="arpege_2D_avg_cloud_variables" >
        <field id="clt"		long_name="cloud_area_fraction" 					unit="1"/>
        <field id="clt100"	long_name="cloud_area_fraction"						unit="%" field_ref="clt"> clt * 100 </field>
        <field id="cltc"	long_name="convective_cloud_area_fraction"				unit="1"/>
        <field id="cltc100"	long_name="convective_cloud_area_fraction"				unit="%" field_ref="cltc"> cltc * 100 </field>
        <field id="clth"	long_name="cloud_area_fraction_in_atmosphere_layer_high"		unit="1"/>
        <field id="clth100"	long_name="cloud_area_fraction_in_atmosphere_layer_high"		unit="%" field_ref="clth"> clth * 100 </field>
        <field id="cltm"	long_name="cloud_area_fraction_in_atmosphere_layer_middle"		unit="1"/>
        <field id="cltm100"	long_name="cloud_area_fraction_in_atmosphere_layer_middle"		unit="%" field_ref="cltm"> cltm * 100 </field>
        <field id="cltl"	long_name="cloud_area_fraction_in_atmosphere_layer_low"			unit="1"/>
        <field id="cltl100"	long_name="cloud_area_fraction_in_atmosphere_layer_low"			unit="%" field_ref="cltl"> cltl * 100 </field>
        <field id="lwp"		long_name="atmosphere_mass_content_of_cloud_liquid_water"		unit="kg m-2"/>
        <field id="iwp"		long_name="atmosphere_mass_content_of_cloud_ice_water"			unit="kg m-2"/>
        <field id="lwpc"	long_name="atmosphere_mass_content_of_convective_cloud_liquid_water"	unit="kg m-2"/>
        <field id="iwpc"	long_name="atmosphere_mass_content_of_convective_cloud_ice_water"	unit="kg m-2"/>
        <field id="lwps"	long_name="atmosphere_mass_content_of_stratiform_cloud_liquid_water"	unit="kg m-2"/>
        <field id="iwps"	long_name="atmosphere_mass_content_of_stratiform_cloud_ice_water"	unit="kg m-2"/>
