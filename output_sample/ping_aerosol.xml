   <field id="CMIP6_convcl"        field_ref="dummy_XYA"        /> <!-- (1.0) convective_cloud_area_fraction_in_atmosphere_layer : Convective cloud fraction in an atmospheric layer -->
   <field id="CMIP6_convclt"       field_ref="dummy_XY"         /> <!-- (1.0) convective_cloud_area_fraction : Convective cloud fraction -->
